<br/><br/><br/><br/>
    <div class='sel_banner'>
      <p class="total_member hidden"><%=@total_member%></p>
    </div>
    <table class="table table-bordered table-hover">
      <tr>
        <td>会员ID</td>
        <td>SID</td>
        <td>姓名</td>
        <td>手机</td>
        <td>注册时间</td>
        <th colspan="3">操作</th>
      </tr>

      <% @visitors.each do |visitor| %>
          <tr>
            <td><%=visitor.user.member_id %></td>
            <td><%=visitor.user.account.login_name%></td>
            <td><%=visitor.user.name %></td>
            <td><%=visitor.user.mobile%></td>
            <td><%=Time.at(visitor.user.account.createtime).strftime('%Y-%m-%d %H:%M') %></td>
            <td><%=link_to "查看",info_admin_member_path(visitor.user.member_id)%></td>
          </tr>
      <% end %>
    </table>

    <div class="pagination">
      <%= will_paginate @visitors, :container => false %>
    </div>

