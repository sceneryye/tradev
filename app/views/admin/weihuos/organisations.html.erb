  <h1>尾货良品- 组织管理</h1>
<table class="table table-bordered table-striped table-hover">
  <thead>
  <tr>
    <th>id</th>
    <th>名称</th>
    <th>上一级</th>
    <th>分润率</th>
    <th>级别</th>
    <th>是否终端</th>
    <th>负责人</th>
    <th>电话</th>
    <th>地址</th>
    <th>新建时间</th>
    <th>修改时间</th>
  </tr>
  </thead>
  <tbody>
  <%@organisations.each do |organisation|%>
  <tr>
    <td><%=organisation.id%></td>
    <td><%=organisation.name%></td>
    <td><%=organisation.path%>
    <td><%=organisation.share%></td>
    <td><%=organisation.level%></td>
    <td><%=organisation.is_leaf%></td>
    <td><%=organisation.manager%></td>
    <td><%=organisation.tel%></td>
    <td><%=organisation.address%></td>
    <td><%=organisation.created_at%></td>
    <td><%=organisation.updated_at%></td>
  </tr>
  <%end if @organisations%>
  </tbody>
</table>
<div class="paginations">
  <%=will_paginate @organisations,:container=>false if @organisations%>
</div>
